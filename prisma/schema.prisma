generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id    String  @id @unique
  name  String
  email String
  image String
  links Links[]
}

model Links {
  id          String        @id @unique
  name        String
  userId      String
  user        Users         @relation(fields: [userId], references: [id])
  connections Connections[]
}

model Connections {
  id     String @id @unique
  linkId String
  link   Links  @relation(fields: [linkId], references: [id])
}
